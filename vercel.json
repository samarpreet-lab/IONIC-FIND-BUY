{
  "buildCommand": "npm run build",
  "outputDirectory": "www",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*\\.js)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.css)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.json)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.svg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.png)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.jpg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.jpeg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.gif)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.webp)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.woff)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.woff2)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.ttf)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.eot)",
      "destination": "/$1"
    },
    {
      "source": "/(?!.*\\.)",
      "destination": "/index.html"
    }
  ]
}
